{
  "entrypoint": "src/pb-components.js",
  "npm": true,
  "moduleResolution": "node",
  "sources": [
    "src/*.js",
    "package.json"
  ],
  "builds": [
    {
      "bundle": true,
      "js": {
        "minify": true,
        "compile": "es5",
        "transformModulesToAmd": true
      },
      "addServiceWorker": false,
      "addPushManifest": false
    }
  ]
}